scalar LocalDateTime

type Query {
    patients: [Patient]
}

type Patient {
    id: ID
    name: String
    bodyMeasure: BodyMeasure
    spO2: [SpO2]
}

type BodyMeasure {
    id: ID
    patientId: Int
    height: Int
    weight: Int
    time: LocalDateTime
}

type SpO2 {
    id: ID
    patientId: Int,
    percentage: Int,
    time: LocalDateTime
}